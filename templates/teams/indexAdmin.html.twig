{% extends 'baseAdmin.html.twig' %}

{% block title %}Teams{% endblock %}

{% block body %}
    <h1>Teams</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>TeamName</th>
            <th>TeamTag</th>
            <th>TeamMail</th>
            <th>TeamReg</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for team in teams %}
            <tr>
                <td>{{ team.id }}</td>
                <td>{{ team.teamName }}</td>
                <td>{{ team.teamTag }}</td>
                <td>{{ team.teamMail }}</td>
                <td>{{ team.teamReg }}</td>
                <td>
                    <a href="{{ path('app_teams_show', {'id': team.id}) }}">show</a>

                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
