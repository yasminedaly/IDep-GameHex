<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Games" table="games">
    <id name="contentid" type="integer" column="contentID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contenttitle" type="string" column="contentTitle" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contentdate" type="date" column="contentDate" nullable="false"/>
    <field name="gamingcontent" type="blob" column="gamingContent" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
